{
  "name": "telegram-audio-discord",
  "description": "Display Telegram music playback as Discord Rich Presence",
  "version": "1.0.0",
  "module": "index.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun --watch index.ts",
    "tray": "bun run tray.ts",
    "build": "bun build --compile --minify tray.ts --outfile telegram-discord-presence",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/discord-rpc": "^4.0.10"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@xhayper/discord-rpc": "^1.3.0",
    "discord-rpc": "^4.0.1",
    "systray2": "^2.1.4"
  },
  "trustedDependencies": [
    "register-scheme"
  ]
}
